			<display_entry name="dDysenteryRisk" title_key="statHarvestQuantitySalvage" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDysenteryRisk" title_key="statHarvestQuantityMeat" display_type="Percent" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>			
		</item_display_info>
	</append>
</Dementive>