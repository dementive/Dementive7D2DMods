<Dementive>
	<append xpath="ui_display_info/item_display">
		<item_display_info display_type="grapeCrush" display_group="groupConsumables">
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>		
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>			
			<display_entry name="dDysenteryRisk" title_key="statJumpDistance" display_leading_plus="true"/>
			<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>			
		</item_display_info>
		<item_display_info display_type="appleCrush" display_group="groupConsumables">
			<display_entry name="$waterAmountAdd" title_key="statWaterAmount"/>
			<display_entry name="dStaminaRegen" title_key="statStaminaRegen" display_type="Percent"/>
			<display_entry name="dLootStageFlat" title_key="statLootStage" display_leading_plus="true"/>
			<display_entry name="dLootStagePerc" title_key="statLootStage" display_type="Percent" display_leading_plus="true"/>			
